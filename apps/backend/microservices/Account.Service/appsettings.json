{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost,1433;Database=AccountDb;User Id=sa;Password=${MSSQL_SA_PASSWORD};TrustServerCertificate=true;"
  },
  "JwtSettings": {
    "SecretKey": "${JWT_SECRET_KEY}",
    "Issuer": "Account.Service",
    "Audience": "Pogo.Community",
    "ExpiryHours": 12
  }
}
