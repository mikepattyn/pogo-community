{
  "name": "pogo-community-monorepo",
  "version": "1.0.0",
  "description": "POGO Community monorepo with API, Mobile App, and Discord Bot",
  "private": true,
  "workspaces": [
    "apps/backend/*",
    "apps/frontend/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean && rm -rf node_modules",
    "type-check": "turbo run type-check"
  },
  "devDependencies": {
    "turbo": "^1.10.12",
    "typescript": "^5.0.2"
  },
  "packageManager": "pnpm@8.6.10",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "pnpm": {
    "overrides": {
      "inversify": "^6.2.2",
      "reflect-metadata": "^0.2.2"
    }
  }
}
